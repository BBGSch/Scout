<div class="container container-new">
  <div class="row">
    <div class="col-6">



    <h1>TrainingSessions#index</h1>
    <p>Find me in app/views/training_sessions/index.html.erb</p>

    <% @trainingsessions.each do |sesh| %>



      <h3><%= link_to sesh.training.name, training_session_path(sesh) %></h3>
      <h5><%= sesh.time %></h5>
      <h5><%= sesh.location %></h5>
      <h5><%= sesh.capacity %></h5>

    <% end %>

    </div>
  </div>
  <!-- Uncomment in order to enable the map -->
  <div><%#= render "map",  markers: @markers %></div>
</div>


